@model List<Marka>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div class="list-group">
    <ul class="list-group">
        @foreach(Marka marka in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Model" asp-action="Index" asp-route-data="@marka.Id">
                    @marka.MarkaName
            </a></li>
        }
    </ul>
</div>

